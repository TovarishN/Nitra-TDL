deployment D(string title, bool isVerbose = false)
{
  KL.Autotests.Commands.LogMessage(Message: title);
  KL.Autotests.Commands.ComplexOperation(IsVerbose: isVerbose, Result: out var result);
  KL.Autotests.Commands.LogMessage(Severity: 2, Message: result);
  expected 0;
}

suite Android
{
    platform w19-droid;
    product MobileSdk;
    title = "C# deployment";
    AndroidJavaTestGroupKavsdk();
}

scenario AndroidJavaTestGroupKavsdk
{
    deployment D;
    environments (w19-droid, MobileSdk);
    apk "KavSdk-full-debug-androidTest.apk"
        filter "-e pPdkDeliveryTest true -e pPullRequestTest true"
        runner "com.kavsdk.test/com.kavsdk.test.TestRunner";
    UniqueId = @"bccd157e-ca69-4304-a3af-ae0c5cb0eea1";
    NeedUnsignedAgent = true;
    Timeout = @"00:10:00";    
}

platform w19-droid;

product type Kavkis
{
    string Name;
    string BranchName;
    string Version;
}

product MobileSdk : Kavkis 
{
    Name = @"MobileSdk";
    BranchName = @"1";
    Version = @"1.0.0.0";
}