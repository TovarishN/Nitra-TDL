{
    Name = @"PURE";
    ReleaseName = @"Main";
    Version = ProductVersion;
    AutotestBinariesExcludedFilesMasks = @"ksm*.Product.Config;ksde*.Product.Config";
    ProductBinariesExcludedFilesMasks = @"*_full.exe;*_reduced.exe";
