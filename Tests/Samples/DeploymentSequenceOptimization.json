{
  "version": 7,
  "$schema": "http://auto-reports.avp.ru/starter-schema/v7/prod/schema.json",
  "Deployments": {
    "D1": {
      "Type": "Script",
      "ReturnValue": 0,
      "ScriptArgs": {
        "Message": "$(Message)"
      },
      "ScriptArgsDefaultValues": {
        "Message": "#(Foo)"
      },
      "ScriptData": "",
      "ScriptDataExtension": ".ps1"
    },
    "D2": {
      "Type": "Script",
      "ReturnValue": 0,
      "ScriptArgs": {
        "IsVerbose": "$(IsVerbose)"
      },
      "ScriptArgsDefaultValues": {
        "IsVerbose": 0
      },
      "ScriptData": "",
      "ScriptDataExtension": ".ps1"
    },
    "D3": {
      "Type": "Script",
      "ScriptArgs": {},
      "ScriptArgsDefaultValues": {},
      "ScriptPath": "file.ps1"
    },
    "S1": {
      "Type": "Script",
      "ReturnValue": 0,
      "ScriptArgs": {
        "Message": "$(Message)",
        "IsVerbose": "$(IsVerbose)"
      },
      "ScriptArgsDefaultValues": {
        "Message": "#(Foo)",
        "IsVerbose": 0
      },
      "ScriptData": "",
      "ScriptDataExtension": ".ps1"
    },
    "D1 >>>> D2": {
      "Type": "Script",
      "ReturnValue": 0,
      "ScriptArgs": {
        "Message": "$(Message)",
        "IsVerbose": "$(IsVerbose)"
      },
      "ScriptArgsDefaultValues": {
        "Message": "#(Foo)",
        "IsVerbose": 0
      },
      "ScriptData": "",
      "ScriptDataExtension": ".ps1"
    },
    "S2": {
      "Type": "Sequence",
      "Deployments": [
        "D1 >>>> D2",
        "D3"
      ]
    },
    "S3": {
      "Type": "Sequence",
      "Deployments": [
        "D3",
        "D1 >>>> D2"
      ]
    },
    "S4": {
      "Type": "Sequence",
      "Deployments": [
        "D1",
        "D3",
        "D2"
      ]
    },
    "SS1 |||| X": {
      "Type": "Script",
      "ReturnValue": 0,
      "ScriptArgs": {
        "Message": "$(Message)",
        "IsVerbose": "$(IsVerbose)"
      },
      "ScriptArgsDefaultValues": {
        "Message": "#(Foo)",
        "IsVerbose": 0
      },
      "ScriptData": "",
      "ScriptDataExtension": ".ps1"
    },
    "SS1": {
      "Type": "Select",
      "Condition": "$(F)",
      "Values": {
        "X": "SS1 |||| X"
      }
    },
    "SS2 |||| X": {
      "Type": "Sequence",
      "Deployments": [
        "D1 >>>> D2",
        "D3"
      ]
    },
    "SS2": {
      "Type": "Select",
      "Condition": "$(F)",
      "Values": {
        "X": "SS2 |||| X"
      }
    },
    "SS3 |||| X": {
      "Type": "Sequence",
      "Deployments": [
        "D3",
        "D1 >>>> D2"
      ]
    },
    "SS3": {
      "Type": "Select",
      "Condition": "$(F)",
      "Values": {
        "X": "SS3 |||| X"
      }
    },
    "SS4 |||| X": {
      "Type": "Sequence",
      "Deployments": [
        "D1",
        "D3",
        "D2"
      ]
    },
    "SS4": {
      "Type": "Select",
      "Condition": "$(F)",
      "Values": {
        "X": "SS4 |||| X"
      }
    }
  },
  "Platforms": {},
  "Products": {},
  "Suites": {},
  "TestScenarios": {},
  "TestEntities": {}
}

