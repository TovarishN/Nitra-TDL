        "UpdateSource": "",
        "UseCollections": "On",
        "UseGDPR": "Default",
        "UseProxyForUpdate": false,
        "Verbose": false,
        "VerboseExternalProcessesOutput": false,
        "WaitForProductActivation": false
      },
      "ScriptPath": "\\\\tis-dfs.company.ru\\Autotests\\App2\\DeploymentScripts\\Deployment\\deploy.ps1"
    },
    "Install 2019 -> Install product NoBackdoor TraceLevel_1000": {
      "Type": "Sequence",
      "Deployments": [
        "Install Base Product 2019",
        "_Install product NoBackdoor TraceLevel_1000",
        "RebootAgent"
      ]
    },
    "Install Base Product 2019": {
      "Type": "Sequence",
      "Deployments": [
        {
          "Install Base Product": {
            "AdditionalFiles": "\\\\tis-dfs.company.ru\\Autotests\\App2\\AdditionalFiles\\App2_2019.MP0.AdditionalContinuousIntegration"
          }
        }
      ]
    },
    "Install Base Product": {
      "Type": "Sequence",
      "Deployments": [
        {
          "_Install product": {
            "BuildPath": "$(SessionWorkFolder)\\BaseProductBuild",
            "DisableTestModulesInstallation": true,
            "DisableVSCollectorsDeployment": true,
            "InstallActivationServerBackdoor": true,
            "SetProductNameForFacade": false,
            "StartService": true,
            "TurnOffDumpOnAssert": true,
            "UseCollections": "Off"
          }
        }
      ]
    }
  },
