        "VerboseExternalProcessesOutput": "$(VerboseExternalProcessesOutput)",
        "WaitForProductActivation": "$(WaitForProductActivation)"
      },
      "ScriptArgsDefaultValues": {
        "BuildPath": "$(SessionWorkFolder)\\ProductBuild",
