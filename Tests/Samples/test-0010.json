        "IgnoreNothingToUpdateCode": false,
        "InstallActivationServerBackdoor": false,
        "InstallerExecutableFileMask": "",
        "InstallLib1": false,
        "InstallLicenceBackdoor": "On",
        "IsoImageFileName": "",
        "KpcActivationCode": "",
        "KpcCompanyId": "",
        "KpcOneTimeRegistrationSharedSecret": "",
        "KPCRegion": "",
        "KpcUserId": "",
        "LocalizationCode": "",
        "PostUpdateServiceStartStop": false,
