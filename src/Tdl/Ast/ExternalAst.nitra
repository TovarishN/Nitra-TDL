using DotNet;
using DotNet.AstUtils;

using Nitra;
using Nitra.Declarations;

namespace Tdl
{
  ast ExternalAst : NamespaceMember
  {
    Fields.ContainingTable = context.GetExternalSymbolTable();
    Fields.Scope = Scope;

    SetDeclaredIn(context, Fields.Symbol);
    
    Fields : Field*;
  }
}
