using DotNet;
using DotNet.AstUtils;

using Nitra;
using Nitra.Declarations;

namespace Tdl
{
  declaration External : NamespaceMember, Container, TypedDeclaration, Type
  {
    symbol
    {
      Scope     = MemberTable;
      Kind      = "external";
      SpanClass = TdlLang.ExternalSpanClass;
      CanMerge  = true;
    }

    Fields.ContainingTable = Symbol.MemberTable;
    Fields.Scope           = Scope;
    Symbol.Type            = Symbol;

    Fields : Field*;
  }
}
