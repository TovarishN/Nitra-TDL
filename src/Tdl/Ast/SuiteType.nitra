using DotNet;
using DotNet.AstUtils;

using Nitra;
using Nitra.Declarations;

namespace Tdl
{
  ignore-case declaration SuiteType : NamespaceMember, Container
  {
    symbol
    {
      Scope     = MemberTable;
      Kind      = "scenario type";
      SpanClass = TdlLang.TdlTypeSpanClass;
    }

    Fields.ContainingTable = Symbol.MemberTable;
    Fields.Scope = Scope;
    
    Fields : Field*;
  }
}
