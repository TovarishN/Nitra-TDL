using DotNet;
using DotNet.AstUtils;

using Nitra;
using Nitra.Declarations;

using System;
using System.Collections.Immutable;

namespace Tdl
{
  ast From : ScopedAst, BindableAst
  {
    out Type : TypeSymbol;
    
    Vars.ContainingTable = ContainingTable;
    List.Scope           = Scope;
    Type                 = context.MakeTuple(Vars.Type);
    List.ExpectedType    = Type;

    Vars : Var*;
    List : Elem*;
  }
}
